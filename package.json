{
  "name": "personal-bio-site",
  "version": "1.0.0",
  "description": "This project is to be a personal hub site with information/links to projects I have done.  Designed to be a \"single page application\"",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "lint": "eslint ./src --ext .js",
    "dev": "NODE_ENV=development webpack-dev-server --content-base dist --hot",
    "start": "npm run lint && npm run dev",
    "projects": "json-server db/personalProjects.json --port 3003",
    "deploy": "npm run build && firebase deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rfishernc/personal-bio-site.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rfishernc/personal-bio-site/issues"
  },
  "homepage": "https://github.com/Rfishernc/personal-bio-site#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.9",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.8.0",
    "node-sass": "^4.7.2",
